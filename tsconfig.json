{
  "compilerOptions": {
    "skipLibCheck": true,
    "target": "es2015",
    "strict": true,
    "downlevelIteration": true,
    "noImplicitAny": true,
    "noUnusedParameters": false,
    "lib": [
      "es2019"
    ],
    "noEmit": true
  },
  "exclude": [
    "node_modules",
    "src/routes/*"
  ],
  "include": [
    "src/**/*"
, "index.ts"  ]
}