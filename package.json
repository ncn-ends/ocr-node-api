{
  "name": "ocr-node-api",
  "version": "1.0.0",
  "main": "index.ts",
  "license": "MIT",
  "scripts": {
    "run": "babel-node index.ts --extensions \".ts\"",
    "dev": "NODE_ENV=DEV nodemon --exec yarn run run -- index.ts",
    "build": "babel index.ts --out-dir dist --copy-files --extensions \".ts,tsx,html,css\" --source-maps inline",
    "start": "NODE_ENV=PROD node dist/index.js",
    "tsc": "tsc --noEmit",
    "ngrok": "cd ~/Documents && ./ngrok http 3000",
    "debug": "yarn run dev --inspect-brk",
    "createApiKey": "babel-node src/scripts/createKeyScript.ts --extensions \".ts\"",
    "prepare": "husky install",
    "pm2:start": "pm2 start yarn --watch --interpreter bash --name app -- start",
    "test:cypress": "cypress run",
    "test:e2e": "start-server-and-test test:server http://localhost:3000 test:cypress",
    "test:jest": "jest",
    "test:all": "yarn test:e2e && yarn test:jest",
    "test:server": "NODE_ENV=TEST babel-node index.ts --extensions \".ts\"",
    "ssl-gen": "./src/ssl-gen.sh"
  },
  "devDependencies": {
    "@babel/cli": "^7.16.0",
    "@babel/core": "^7.16.0",
    "@babel/node": "^7.16.0",
    "@babel/preset-env": "^7.16.0",
    "@babel/preset-typescript": "^7.16.0",
    "@types/cypress": "^1.1.3",
    "@types/formidable": "^1.2.4",
    "@types/lodash": "^4.14.176",
    "@types/node": "^14.17.6",
    "axios": "^0.24.0",
    "babel-plugin-tsconfig-paths-module-resolver": "^1.0.3",
    "concurrently": "^6.3.0",
    "cypress": "^9.0.0",
    "cypress-file-upload": "^5.0.8",
    "husky": "^7.0.0",
    "jest": "^27.3.1",
    "jest-dev-server": "^6.0.0",
    "nodemon": "^2.0.14",
    "regenerator-runtime": "^0.13.9",
    "start-server-and-test": "^1.14.0",
    "typescript": "^4.4.4"
  }
}
